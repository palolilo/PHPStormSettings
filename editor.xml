<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="HACKED">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="c01012" />
      <option name="FONT_TYPE" value="1" />
    </pattern>
  </component>
</application>
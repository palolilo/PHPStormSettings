<application>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bdump\b.*">
      <option name="FOREGROUND" value="c00015" />
      <option name="BACKGROUND" value="181818" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bHACKED\b.*">
      <option name="FOREGROUND" value="faffff" />
      <option name="BACKGROUND" value="c00015" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bSection\b.*">
      <option name="FOREGROUND" value="181818" />
      <option name="BACKGROUND" value="ecffa1" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <filter name="Check dump() in files">
      <pattern index="2" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>